package treinamento;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner ler = new Scanner(System.in);
		
		String nome;
		
		System.out.println("nome");
		nome = ler.next()
		
		   int y = 1;
	       do{
	            System.out.println("Do; nosso nome é " + nome + y );
	            y++;
	            
	        }while(y <= 10);
       }
	}

}
